   "source": [
    "import sys\n",
    "import statistics\n",
    "\n",
    "nums = [int(sys.stdin.readline().rstrip()) for _ in range(int(input()))]\n",
    "nums.sort()\n",
    "print(round(statistics.mean(nums)))\n",
    "print(statistics.median(nums))\n",
    "_n = statistics.multimode(nums)\n",
    "print(_n[1 if len(_n) != 1 else 0])\n",
    "print(max(nums) - min(nums))"
   ]
