   "source": [
    "x, y = map(int, input().split())\n",
    "z = int((y * 100) / x)\n",
    "k1 = (x * (z + 1)) - (100 * y)\n",
    "k2 = 99 - z\n",
    "\n",
    "if z >= 99: print(-1)\n",
    "else:\n",
    "    if k1 % k2 == 0: print(k1 // k2)\n",
    "    else: print((k1 // k2) + 1)"
   ]
